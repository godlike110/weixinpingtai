<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>
    <rule>
        <from>^/question/show/(\d+)/(\d+)$</from>
        <to>/ask/$2?from=wap&amp;groupId=$1</to>
    </rule>

    <rule>
        <from>^/q(\d+)$</from>
        <to>/ask/$1</to>
    </rule>

    <rule>
        <from>^/b(\d+)$</from>
        <to>/news/baodian/$1</to>
    </rule>

    <rule>
        <from>^/$</from>
        <to type="forward">/home</to>
    </rule>

    <rule>
        <from>^/appxinfang/wap$</from>
        <to>/static/appwap.html</to>
    </rule>

    <rule>
        <from>^/appxinfang$</from>
        <to>/static/appxinfang.html</to>
    </rule>

    <rule>
        <from>^/app/pinge$</from>
        <to>/static/pinge.html</to>
    </rule>

    <rule>
        <from>^/app/pinge/wap$</from>
        <to>/static/pingewap.html</to>
    </rule>
    
    <rule>
    	<from>^/sohunews/zixun(.*)$</from>
    	<to>/sohunews/quiz$1</to>
    </rule>
    
</urlrewrite>